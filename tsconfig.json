{
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "paths": {
      "~/*": ["./dun/frontend/src/*"],
      "@dun/assets/*": ["./dun/frontend/assets/*"],
      "@dun/decl": ["./declarations/dun/index.js"]
    }
  }
}
